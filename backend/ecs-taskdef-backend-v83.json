{
  "family": "backend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::851725596604:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::851725596604:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "851725596604.dkr.ecr.eu-central-1.amazonaws.com/backend:v83",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "eu-central-1"
        },
        {
          "name": "SQS_QUEUE_URL",
          "value": "https://sqs.eu-central-1.amazonaws.com/851725596604/proov-worker-queue"
        },
        {
          "name": "JWT_SECRET_KEY",
          "value": "proov-jwt-secret-key-2025-change-in-production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/backend",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
