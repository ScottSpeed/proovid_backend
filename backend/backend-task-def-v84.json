{
    "family": "backend-task",
    "taskRoleArn": "arn:aws:iam::851725596604:role/ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::851725596604:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "4096",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "851725596604.dkr.ecr.eu-central-1.amazonaws.com/backend:v84",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "eu-central-1"
                },
                {
                    "name": "JOB_TABLE",
                    "value": "proov_jobs"
                },
                {
                    "name": "SQS_QUEUE_URL",
                    "value": "https://sqs.eu-central-1.amazonaws.com/851725596604/proov-worker-queue"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/backend",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
