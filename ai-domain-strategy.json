{
    "family": "backend-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::851725596604:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "851725596604.dkr.ecr.eu-central-1.amazonaws.com/backend:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/backend-task",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "environment": [
                {"name": "FRONTEND_ORIGINS", "value": "https://proovid.ai,https://localhost:5173"},
                {"name": "BEDROCK_TIMEOUT", "value": "60.0"}
            ]
        }
    ]
}